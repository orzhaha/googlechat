{
  "name": "extension",
  "description": "google chat extension",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "all_frames":true,
      "run_at":"document_start"
    }
  ],
  "manifest_version": 2
}
